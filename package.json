{
  "name": "writr",
  "version": "0.9.5",
  "description": "A Simple to Use Markdown Blog",
  "main": "./dist/index",
  "types": "./dist/index",
  "repository": "https://github.com/jaredwray/writr.git",
  "author": "Jared Wray <me@jaredwray.com>",
  "license": "MIT",
  "keywords": [
    "static-site-generator",
    "static-site",
    "ssg",
    "documentation",
    "website",
    "blog",
    "templates",
    "generator",
    "framework",
    "writr",
    "writer",
    "html",
    "markdown",
    "handlebars"
  ],
  "scripts": {
    "watch": "tsc -w -p .",
    "clean-all": "yarn clean && rm -rf node_modules && rm -rf coverage",
    "clean": "rm -rf ./dist && rm -rf ./blog_output",
    "compile": "yarn clean && tsc -p .",
    "build": "yarn test && yarn compile",
    "test": "jest --coverage",
    "test-output": "yarn compile && node ./bin/writr -p ./blog_example",
    "coverage": "codecov -f coverage/*.json -t 62b3983c-08a1-425f-99b9-71e84fef74bc"
  },
  "bin": {
    "writr": "./bin/writr"
  },
  "dependencies": {
    "array-sort": "^1.0.0",
    "cheerio": "^1.0.0-rc.3",
    "commander": "^6.2.0",
    "feed": "^4.2.1",
    "fs-extra": "^9.0.1",
    "gray-matter": "^4.0.2",
    "handlebars": "4.7.6",
    "helper-date": "^1.0.1",
    "keyv": "^4.0.3",
    "markdown-it": "^12.0.2",
    "moment": "^2.29.1",
    "parse-json": "^5.1.0",
    "promisify": "^0.0.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/array-sort": "^1.0.0",
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.9",
    "@types/express-serve-static-core": "^4.17.13",
    "@types/fs-extra": "^9.0.4",
    "@types/handlebars": "^4.0.37",
    "@types/keyv": "^3.1.1",
    "@types/markdown-it": "^10.0.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.9",
    "@types/parse-json": "^4.0.0",
    "@types/winston": "^2.3.8",
    "codecov": "^3.8.1",
    "del": "^6.0.0",
    "jest": "^26.6.3",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "files": [
    "dist",
    "bin"
  ]
}
